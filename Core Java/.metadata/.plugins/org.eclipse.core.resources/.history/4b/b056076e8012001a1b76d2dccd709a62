



package com.nobel.model;

public class Employees {
	private int id;
	private String name;
	private String location;
	private int sallary;
	private String managerName;

	public Employees(int id, String name, String location, int sallary,String managerName) {
		super();
		this.id = id;
		this.name = name;
		this.location = location;
		this.sallary = sallary;
		this.managerName=managerName;
	}

	@Override
	public String toString() {
		
		return "Employees [id=" + id + ", name=" + name + ", location=" + location + ", sallary=" + sallary + "]";
		
	}
	
}
