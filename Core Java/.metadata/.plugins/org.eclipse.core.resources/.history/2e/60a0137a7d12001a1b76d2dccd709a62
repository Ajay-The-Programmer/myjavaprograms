//1. Get employees whose salary is greater than given value ( 50,000)
//2. Employess whos salary between given range ( 40K - 70K)
//3. Employee based on given location
//4. Update salary of all employees by 10% whose location is Pune.




package com.nobel.model;

public class Employees {
	private int id;
	private String name;
	private String location;
	private int sallary;
	private String managerName;

	public Employees(int id, String name, String location, int sallary,String managerName) {
		super();
		this.id = id;
		this.name = name;
		this.location = location;
		this.sallary = sallary;
		this.managerName=managerName;
	}

	@Override
	public String toString() {
		
		return "Employees [id=" + id + ", name=" + name + ", location=" + location + ", sallary=" + sallary + "]";
		
	}
	
}
